public interface  org.xmpp.component.Component extends java.lang.Object
{

    public abstract java.lang.String getName();

    public abstract java.lang.String getDescription();

    public abstract void processPacket(org.xmpp.packet.Packet);

    public abstract void initialize(org.xmpp.packet.JID, org.xmpp.component.ComponentManager) throws org.xmpp.component.ComponentException;

    public abstract void start();

    public abstract void shutdown();
}
