{

    public abstract void startRecord(int);

    public abstract void setNextRecordSize(int);

    public abstract void skipTwoBytes();

    public abstract void xor(byte[], int, int);

    public abstract int xorByte(int);

    public abstract int xorShort(int);

    public abstract int xorInt(int);

    public abstract long xorLong(long);
}
