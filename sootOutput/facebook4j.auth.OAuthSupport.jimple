public interface  facebook4j.auth.OAuthSupport extends java.lang.Object
{

    public abstract void setOAuthAppId(java.lang.String, java.lang.String);

    public abstract void setOAuthPermissions(java.lang.String);

    public abstract java.lang.String getOAuthAuthorizationURL(java.lang.String);

    public abstract java.lang.String getOAuthAuthorizationURL(java.lang.String, java.lang.String);

    public abstract facebook4j.auth.AccessToken getOAuthAccessToken();

    public abstract facebook4j.auth.AccessToken getOAuthAccessToken(java.lang.String) throws facebook4j.FacebookException;

    public abstract facebook4j.auth.AccessToken getOAuthAppAccessToken() throws facebook4j.FacebookException;

    public abstract void setOAuthAccessToken(facebook4j.auth.AccessToken);
}
