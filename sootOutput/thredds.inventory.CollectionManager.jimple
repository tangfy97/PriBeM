public interface  thredds.inventory.CollectionManager extends java.lang.Object
{

    public abstract java.lang.String getCollectionName();

    public abstract void scan(ucar.nc2.util.CancelTask) throws java.io.IOException;

    public abstract boolean isRescanNeeded();

    public abstract boolean rescan() throws java.io.IOException;

    public abstract void resetProto();

    public abstract ucar.nc2.units.TimeUnit getRecheck();

    public abstract long getLastScanned();

    public abstract long getLastChanged();

    public abstract java.lang.String getRoot();

    public abstract java.util.List getFiles();

    public abstract java.util.Date extractRunDate(thredds.inventory.MFile);

    public abstract void close();

    public abstract void putMetadata(thredds.inventory.MFile, java.lang.String, byte[]);

    public abstract byte[] getMetadata(thredds.inventory.MFile, java.lang.String);
}
