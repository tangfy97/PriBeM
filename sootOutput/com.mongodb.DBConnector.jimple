    public abstract void requestDone();

    public abstract void requestEnsureConnection();

    public abstract com.mongodb.WriteResult say(com.mongodb.DB, com.mongodb.OutMessage, com.mongodb.WriteConcern) throws com.mongodb.MongoException;

    public abstract com.mongodb.Response call(com.mongodb.DB, com.mongodb.DBCollection, com.mongodb.OutMessage) throws com.mongodb.MongoException;

    public abstract com.mongodb.Response call(com.mongodb.DB, com.mongodb.DBCollection, com.mongodb.OutMessage, int) throws com.mongodb.MongoException;
}
