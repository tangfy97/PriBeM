public interface  facebook4j.FacebookBase extends java.lang.Object
{

    public abstract java.lang.String getId() throws facebook4j.FacebookException, java.lang.IllegalStateException;

    public abstract java.lang.String getName() throws facebook4j.FacebookException, java.lang.IllegalStateException;

    public abstract facebook4j.auth.Authorization getAuthorization();

    public abstract facebook4j.conf.Configuration getConfiguration();

    public abstract facebook4j.ResponseList fetchNext(facebook4j.Paging) throws facebook4j.FacebookException;

    public abstract facebook4j.ResponseList fetchPrevious(facebook4j.Paging) throws facebook4j.FacebookException;

    public abstract void shutdown();
}
